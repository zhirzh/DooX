{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "data": "yarn data/index.ts",
    "server": "yarn ts:node server/index.ts",
    "server:watch": "nodemon -e ts -w server/ -x 'yarn server'",
    "ts:codegen": "graphql-codegen -c ts-codegen.yml",
    "ts:codegen:watch": "yarn ts:codegen -w",
    "ts:node": "node -r esm -r ts-node/register -r tsconfig-paths/register -r dotenv/config"
  },
  "dependencies": {
    "@babel/core": "~7.9.0",
    "@graphql-codegen/cli": "^1.19.4",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-resolvers": "^1.18.0",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.12",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@types/uuid": "^8.3.0",
    "apollo-server": "^2.19.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "expo": "~40.0.0",
    "expo-status-bar": "~1.0.3",
    "fuse.js": "^6.4.3",
    "graphql": "^15.4.0",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-web": "~0.13.12",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~4.0.0",
    "uuid": "^8.3.2"
  }
}
